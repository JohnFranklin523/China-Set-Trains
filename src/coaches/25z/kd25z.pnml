    return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}

switch (FEAT_TRAINS, SELF, switch_kd25z_graphics_subtype_year_depend, build_year) {
    0..2014: spriteset_kd25z_xizi;//UNfinished
    spriteset_kd25z_xizi;
}
